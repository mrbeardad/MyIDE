{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  // Startup
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "startOnUserLogin": false,

  // Interaction
  "copyOnSelect": true,
  "copyFormatting": true,
  "tabSwitcherMode": "mru",

  // Appearance
  "showTerminalTitleInTitlebar": false,

  // Profile
  "profiles": {
    "defaults": {
      "colorScheme": {
        "dark": "Tango Dark",
        "light": "Tango Light"
      },
      "font": {
        "face": "SauceCodePro Nerd Font"
      },
      "cursorShape": "underscore",
      "intenseTextStyle": "bold",
      "opacity": 50,
      "useAcrylic": true,
      "padding": "0",
      "scrollbarState": "visible",
      "unfocusedAppearance": {
        "opacity": 90
      },
      "autoMarkPrompts": true,
      "showMarksOnScrollbar": true,
      "experimental.repositionCursorWithMouse": true
    },
    "list": [
      {
        "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
        "name": "PowerShell",
        "source": "Windows.Terminal.PowershellCore"
      },
      {
        "guid": "{65763c91-38c0-4789-98e0-fed2a0247aa6}",
        "name": "PowerShell for Administrators",
        "icon": "ms-appx:///ProfileIcons/pwsh-preview.png",
        "commandline": "pwsh.exe",
        "startingDirectory": "%USERPROFILE%",
        "elevate": true
      },
      {
        "guid": "{573f97a4-8cb9-423c-9d41-f16f0586a05a}",
        "name": "PowerShell for VS 2022",
        "icon": "ms-appx:///ProfileIcons/vs-pwsh.png",
        "commandline": "pwsh.exe -NoExit -Command \"& \"\"\"C:\\Program Files\\Microsoft Visual Studio\\2022\\Community\\Common7\\Tools\\Launch-VsDevShell.ps1\"\"\" -Arch amd64 -SkipAutomaticLocation\""
      },
      {
        "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
        "name": "Command Prompt",
        "commandline": "%SystemRoot%\\System32\\cmd.exe"
      },
      {
        "guid": "{5bad30ef-9025-4a38-aae7-6a2699a84c2b}",
        "name": "Git Bash",
        "icon": "%GIT_INSTALL_ROOT%\\usr\\share\\git\\git-for-windows.ico",
        "commandline": "bash.exe -li",
        "startingDirectory": "%USERPROFILE%"
      },
      {
        "guid": "{a453ae62-4e3d-5e58-b989-0a998ec441b8}",
        "name": "Cloud Virtual Machine",
        "commandline": "ssh -t -o ServerAliveInterval=60 ubuntu@43.134.204.157 sh -c 'tmux new-session -s Ubuntu || tmux attach -t Ubuntu'",
        "hidden": true
      },
      {
        "guid": "{b453ae62-4e3d-5e58-b989-0a998ec441b8}",
        "name": "Azure Cloud Shell",
        "source": "Windows.Terminal.Azure"
      }
    ]
  },


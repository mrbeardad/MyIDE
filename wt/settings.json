  // Actions
  "actions": [
    {
      "keys": "win+`",
      "command": {
        "action": "globalSummon",
        "name": "_quake"
      }
    },
    {
      "keys": "alt+shift+v",
      "command": {
        "action": "splitPane",
        "split": "right",
        "splitMode": "duplicate"
      }
    },
    {
      "keys": "alt+shift+s",
      "command": {
        "action": "splitPane",
        "split": "down",
        "splitMode": "duplicate"
      }
    },
    {
      "keys": "alt+shift+z",
      "command": "togglePaneZoom"
    },
    {
      "keys": "ctrl+`",
      "command": {
        "action": "sendInput",
        "input": "\u001b[96;3u"
      }
    },
    {
      "keys": "ctrl+i",
      "command": {
        "action": "sendInput",
        "input": "\u001b[73;4u"
      }
    },
    {
      "keys": "ctrl+shift+j",
      "command": {
        "action": "sendInput",
        "input": "\u001b[74;5u"
      }
    },
    {
      "keys": "ctrl+shift+k",
      "command": {
        "action": "sendInput",
        "input": "\u001b[75;5u"
      }
    },
    {
      "keys": "ctrl+shift+l",
      "command": {
        "action": "sendInput",
        "input": "\u001b[76;5u"
      }
    },
    {
      "keys": "ctrl+enter",
      "command": {
        "action": "sendInput",
        "input": "\u001b[13;5u"
      }
    },
    {
      "keys": "shift+enter",
      "command": {
        "action": "sendInput",
        "input": "\u001b[13;2u"
      }
    }
  ]

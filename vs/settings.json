/* Visual Studio Settings File */
{
  "environment.documents.showMiscFileProject": true,
  "environment.documents.saveWithSpecificEncoding": true,
  "environment.documents.saveEncoding": "utf-8;65001",
  "environment.tabs.documentTabs.showInvisibleTabsInItalic": true,
  "environment.tabs.documentTabs.showPinnedTabsInSeparateRow": true,
  "environment.tabs.documentTabs.dirtyIndicator": "dot",
  "environment.tabs.documentTabs.colorization": "project",
  "environment.tabs.documentTabs.insertRight": true,
  "environment.tasklist.tokens": [
    {
      "text": "HACK",
      "priority": "normal"
    },
    {
      "text": "UNDONE",
      "priority": "normal"
    },
    {
      "text": "UnresolvedMergeConflict",
      "priority": "high"
    },
    {
      "text": "FIXME",
      "priority": "high"
    },
    {
      "text": "PERF",
      "priority": "normal"
    }
  ],
  "environment.terminal.profiles+defaultItemIndex": 0,
  "environment.terminal.profiles": [
    {
      "displayName": "Developer PowerShell",
      "location": "pwsh.exe",
      "arguments": "-NoExit -Command \"& { Import-Module \"\"\"$env:VSAPPIDDIR\\..\\Tools\\Microsoft.VisualStudio.DevShell.dll\"\"\"; Enter-VsDevShell -SkipAutomaticLocation -SetDefaultWindowTitle -InstallPath $env:VSAPPIDDIR\\..\\..\\}\""
    },
    {
      "displayName": "Developer Command Prompt",
      "location": "C:\\WINDOWS\\system32\\cmd.exe",
      "arguments": "/k \"\"%VSAPPIDDIR%\\..\\Tools\\VsDevCmd.bat\""
    }
  ],
  "environment.documents.checkForConsistentLineEndings": false,
  "projectsAndSolutions.locations.newProjectOrSolution": "C:\\Users\\A\\source\\",
  "sourceControl.gitSettings.defaultRepositoryPath": "C:\\Users\\A\\source\\",
  "textEditor.general.audioCues": true,
  "textEditor.general.clickToGoToDefinitionModifierKey": "alt",
  "textEditor.general.display.visibleWhitespace": true,
  "textEditor.general.display.zeroWidthCharacters": true,
  "textEditor.general.display.bracePairColorization": true,
  "textEditor.codeCompletions.general.codeCompletionsProviders": [
    "copilot-completions",
    "copilot-nes",
    "intellicode"
  ],
  "textEditor.codeCompletions.preferences.showCodeCompletionsWhileInDebugMode": true,
  "debugging.general.outputToImmediateWindow": true,
  "languages.defaults.scrollbars.verticalScrollBarMode": "map",
  "languages.cpp.intelliSense.quickInfo.enableQISizeAlignmentAllSymbols": true,
  "languages.cpp.intelliSense.quickInfo.onlineHelpSearchProvider": "https://www.google.com/search?q={0}",
  "languages.cpp.intelliSense.completionList.insertNewlineOnEnter": true,
  "languages.cpp.intelliSense.inlayHints.renderHints": true,
  "languages.cpp.intelliSense.inlayHints.hideAutoKeyword": false,
  "languages.cpp.codeStyle.formatting.general.autoIndentOnTab": true,
  "languages.cpp.codeStyle.formatting.general.defaultFormattingStyle": "visual_studio"
}
